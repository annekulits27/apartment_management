<h1>Branches</h1>

<table>
  <thead>
    <tr>
      <th>Branch Type</th>
      <th>Address</th>
    </tr>
  </thead>
  <tbody>
    <% @branches.each do |branch| %>
      <tr>
        <td><%= branch.branch_type %></td>
        <td><%= branch.address %></td>
        <td><%= link_to 'View', branch %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<br>

<%= link_to 'Back', authenticated_root_path %>

